{
    "builds": [{
        "src": "hostel/wsgi.py",
        "use": "@vercel/python",
        "config": {
             "maxLambdaSize": "15mb",
             "runtime": "python3.9"
             },
        "build": {
            "commands": [
                "pip install sqlite"
            ]
            }
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "/hostel/wsgi.py"
    }]
}